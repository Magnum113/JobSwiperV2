{
    "rewrites": [
        {
            "source": "/assets/(.*)",
            "destination": "/dist/public/assets/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/api"
        }
    ],
    "functions": {
        "api/index.ts": {
            "memory": 1024,
            "maxDuration": 10
        }
    }
}